/* TAB */
.nav-tabs{
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  justify-content: flex-start;
  align-items: center;
  li{
    margin-right: 50px;
    &:last-child{
      margin-right: 0;
    }
  }
}
.tab-pane{
  display: none;
  &.active{
    display: block;
  }
}

/* PRODUCT SLIDER, PRODUCT GRID VIEW */
.product-grid-slider.slick-slider{
  .slick-list{
    margin: 0 -15px;
  }
  .slick-slide{
    padding: 0 15px;
  }
}
.product-grid{
  &.justify-center{
    .row-wrapper{
      justify-content: center;
    }
  }
}

/* SCROLL TO TOP */
.scroll-to-top{
  font-size: 24px;
  position: fixed;
  bottom: 30px;
  right: 30px;
  border: 1px solid;
  width: 30px;
  height: 30px;
  text-align: center;
  line-height: 30px;
  border-radius: 3px;
  z-index: 9;
  background: #fff;
  transition: all 0.3s;
  &.hidden{
    opacity: 0;
    transform: translateY(100px);
  }
}

