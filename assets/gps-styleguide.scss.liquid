        visibility: visible;
        transform: translateY(0);
        -webkit-transform: translateY(0);
        -moz-transform: translateY(0);
        -o-transform: translateY(0);
      }
    }
    .hover-change-img{
      display: block;
      position: relative;
      overflow: hidden;
      img{
        width: 100%;
        height: auto;
        display: block;
        &+img{
          position: absolute;
          top: 0;
          left: 0;
          right: 0;
          bottom: 0;
          opacity: 0 !important;
          visibility: hidden;
          transition: all 500ms;
          transform: translateY(20px);
