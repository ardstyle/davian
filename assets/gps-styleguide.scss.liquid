}
hr{
  margin-top: 1rem;
  margin-bottom: 1rem;
  border: 0;
  border-top: 1px solid rgba(0,0,0,.1);
}
a{
  color: $color_link;
  outline: none;
  box-shadow: none;
  transition: 0.3s;
	-moz-transition: 0.3s;
	-webkit-transition: 0.3s;
  &:hover, &:not([disabled]):hover, &:focus{
    color: $color_link_hover;
    text-decoration: none;
  }
  &:focus{
    box-shadow: none;
  }
  &.line-before{
    position: relative;
    &:before{
      content: "";
      height: 1px;
      width: 0px;
      border-top: 1px solid;
      display: inline-block;
      margin-bottom: 3px;
      transition: width 0.3s;
    }
    &:hover:before{
      width: 30px;
      margin-right: 5px;
    }
  }
  &.rounded{
    border: 1px solid;
    padding: 10px 20px;
    text-decoration: none;
    display: inline-block;
  }
}
.sort-by li a{
  color: $color_link;
  &.active{
    color: $color_link_hover;
  }
}
.rte a:not(.btn){
  border:none;
  padding:0;
}
button, .btn, .shopify-payment-button .shopify-payment-button__button--unbranded, .site-header .minicart-content .btn {
  outline: none;
  box-shadow: none;
  color:$color_btn_primary_text;
  background-color:$color_btn_primary_background;
  border:1px solid $color_btn_primary_border;
  font-size: $typography_button_font_size;
  font-family: $typography_button_font;
  font-style: $typography_button_font_style;
  font-weight: $typography_button_font_weight;
  border-radius: $color_btn_primary_border_radius;
  padding: 0 15px;
  height: 40px;
  transition: all 0.3s;
  &:hover, &:focus, &:not([disabled]):hover{
    box-shadow: none;
    color:$color_btn_primary_text_hover;
    background-color:$color_btn_primary_background_hover;
    border-color: $color_btn_primary_border_hover;
  }
}
a.btn{
  line-height: 38px;
}

.shopify-payment-button .shopify-payment-button__button--unbranded{
  &:hover, &:focus, &:not([disabled]):hover{
    box-shadow: none;
    color:$color_btn_primary_text_hover !important;
    background-color:$color_btn_primary_background_hover !important;
    border-color: $color_btn_primary_border_hover;
  }
}
.btn--secondary, .btn--secondary-accent, .site-header .minicart-content .btn--secondary, .btn-2{
  outline: none;
  box-shadow: none;
  color:$color_btn_secondary_text;
  background-color:$color_btn_secondary_background;
  border:1px solid $color_btn_secondary_border;
  font-size: $typography_button_font_size;
  font-family: $typography_button_font;
  font-style: $typography_button_font_style;
  font-weight: $typography_button_font_weight;
  border-radius: $color_btn_secondary_border_radius;
  padding:0 15px;
  height: 40px;
  transition: all 0.3s;
  display: inline-block;
  &:hover, &:focus, &:not([disabled]):hover{
    box-shadow: none;
    color:$color_btn_secondary_text_hover;
    background-color:$color_btn_secondary_background_hover;
    border-color: $color_btn_secondary_border_hover;
  }
}
a.btn--secondary, a.btn-2{
  line-height: 38px;
}
.btn--link, a.btn--link {
  outline: none;
  box-shadow: none;
  background-color: transparent;
  border: 0;
  margin: 0;
  color: $color_text;
  text-align: left;

  &:not([disabled]):hover,
  &:focus {
    color: $color_text;
    background-color: transparent;
    box-shadow: none;
  }

  .icon {
    vertical-align: middle;
  }
}
.text-link, button.text-link {
  display: inline;
  border: 0 none;
  background: none;
  padding: 0;
  margin: 0;
  outline:none;
  box-shadow:none;
  color: $color_link;
  height: auto;
  &:hover{
    color: $color_link_hover;
    background: none;
  }
  &:focus{
    outline:none;
    box-shadow:none;
  }
}
