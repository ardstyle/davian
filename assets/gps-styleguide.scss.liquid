form{
  label{
    font-size: $typography_form_field_font_size;
    color: $color_form_field_text;
    font-family: $typography_body_text_font;
    font-style: $typography_body_text_font_style;
    font-weight:$typography_body_text_font_weight;
    margin: 0;
    display: block;
    text-transform: uppercase;
    margin: 25px 0 10px;
  }
  &[action="/account/reset"], &[action="/account/activate"]{
    input[type="submit"] {
      margin-top:20px;
    }
  }
  &[action="/account/activate"]{
    text-align:left;
  }
}
input[type="number"] {
	appearance: textfield;
	-moz-appearance: textfield;
	-webkit-appearance: textfield;
}
input[type="number"]::-webkit-outer-spin-button, input[type="number"]::-webkit-inner-spin-button {
  -webkit-appearance: none;
}
input[type="search"]::-webkit-search-decoration,
input[type="search"]::-webkit-search-cancel-button,
input[type="search"]::-webkit-search-results-button,
input[type="search"]::-webkit-search-results-decoration {
  -webkit-appearance:none;
}
input[type="text"], input[type="email"], input[type="password"], input[type="number"], input[type="tel"],input[type="date"],input[type="search"]{
  border: 1px solid $color_form_field_border;
  color: $color_form_field_text;
  font-family: $typography_form_field_font;
  font-size: $typography_form_field_font_size;
  font-style: $typography_form_field_font_style;
  font-weight: $typography_form_field_font_weight;
  background: $color_form_field_background;
  margin: 0;
  padding: 0 17px;
  height: 40px;
  border-radius: $color_form_field_border_radius;
  &:focus{
    outline-color: $color_form_field_border;
  }
  &.input--error{
    color: $color_form_field_text;
    border-color: $color_form_field_error;
  }
  &::-webkit-input-placeholder {
    color: $color_form_field_text;
    opacity: 0.7;
  }
  &::-moz-placeholder {
    color: $color_form_field_text;
    opacity: 0.7;
  }
  &:-ms-input-placeholder {
    color: $color_form_field_text;
    opacity: 0.7;
  }
  &:-moz-placeholder {
    color: $color_form_field_text;
    opacity: 0.7;
  }
}
textarea{
  border: 1px solid $color_form_field_border;
  color: $color_form_field_text;
  font-family: $typography_form_field_font;
  font-size: $typography_form_field_font_size;
  font-style: $typography_form_field_font_style;
  font-weight: $typography_form_field_font_weight;
  background:$color_form_field_background;
  border-radius: $color_form_field_border_radius;
  margin:0;
  padding:10px 17px;
  &:focus{
    outline-color: $color_form_field_border;
    border: 1px solid $color_form_field_border;
  }
  &.input--error{
    color: $color_form_field_text;
    border-color: $color_form_field_error;
    background:$color_form_field_background;
  }
  &::-webkit-input-placeholder {
    color: $color_form_field_text;
    opacity: 0.7;
  }
  &::-moz-placeholder {
    color: $color_form_field_text;
