<ul class="pagination-custom">
  {% if paginate.previous %}
    <li><a href="{{ paginate.previous.url }}" class="pagination-nav pagination-prev"><span>{{ 'general.pagination.previous' | t }}</span></a></li>
  {% endif %}

  {% for part in paginate.parts %}
    {% if part.is_link %}
      <li>
        <a href="{{ part.url }}" title="">{{ part.title }}</a>
      </li>
    {% else %}
      {% if part.title == paginate.current_page %}
        <li class="active"><span>{{ part.title }}</span></li>
      {% else %}
        <li><span>{{ part.title }}</span></li>
      {% endif %}
    {% endif %}
  {% endfor %}

  {% if paginate.next %}
    <li><a href="{{ paginate.next.url }}" class="pagination-nav pagination-next"><span>{{ 'general.pagination.next' | t }}</span></a></li>
  {% endif %}
</ul>
